{
  "name": "family-money",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/icons-react": "^2.0.1",
    "@reduxjs/toolkit": "^1.4.0",
    "antd": "^4.9.4",
    "axios": "^0.18.0",
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "node-less-chokidar": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-app-polyfill": "^0.2.0",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^16.7.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.2.0",
    "redux": "^4.0.1",
    "router-view-dom": "0.0.3"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-css": "node-less-chokidar ./src --skip-source-map",
    "watch-css": "node-less-chokidar src/ src/ --watch",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "lint-staged",
    "jslint:fix": "eslint ./src  --fix --ext .js,.jsx",
    "lint:fix": "npm-run-all -s jslint:fix",
    "precommit": "lint-staged",
    "lintAll": "eslint ./src  --ext .js,.jsx"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint ./src  --ext .js,.jsx",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "eslintIgnore": [
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.12.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.0.4",
    "path": "^0.12.7"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
